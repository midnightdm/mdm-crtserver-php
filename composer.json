{
    "name": "midnightdm/mdm-crt2-server",
    "description": "A daemon accepting NMEA vessel data input via UDP and publishing output to a firestore db as backend for https://www.clintonrivertraffic.com",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/midnightdm/mdm-crt2-server",
            "license": "BSD-1-Clause"
        }
    ],
    "type": "project",
    "license": "BSD-1-Clause",
    "authors": [
        {
            "name": "Mike Winkel",
            "email": "admin@clintonrivertraffic.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-grpc": "*",
        "google/cloud-firestore": "^1.27",
        "minishlink/web-push": "^6.0",
        "phpmailer/phpmailer": "^6.5",
        "clicksend/clicksend-php": "^5.0",
        "curl/curl": "^2.3",
        "google/cloud-storage": "^1.25",
        "google/cloud-text-to-speech": "^1.3",
        "abraham/twitteroauth": "^5.0",
        "nickdnk/graph-sdk": "^7.0"
    },
    "autoload": {
        "psr-4" : {
          "Midnightdm\\Mdm-crt2-server\\" : "src"
        }
      }
}
